{% extends "layout.j2" %}
{% block page_title %}
    - Projects
{% endblock page_title %}
{% block page_content %}
    <div class="bulletlist project">
        {% if projects_list|length< 1 %}
            {% include "comp/empty_notice.j2" %}
        {% else %}
            {% for p in projects_list %}
                {% include "comp/project.j2" %}
            {% endfor %}
        {% endif %}
    </div>
{% endblock page_content %}
